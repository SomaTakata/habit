{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.12.0/.schema/devbox.schema.json",
  "packages": [
    "nodejs@20",
    "pnpm@9",
    "postgresql@16",
    "go@1.22.5",
    "pueue@3.4.1",
    "process-compose@1.9.0",
    "air@1.52.0"
  ],
  "shell": {
    "init_hook": [
      "unset GOROOT GOPATH GOMODCACHE GOBIN GOTOOLDIR",
      "./devbox/init.sh"
    ],
    "scripts": {
      "test": ["echo \"Error: no test specified\" && exit 1"]
    }
  },
  "env": {
    "GOENV": "off",
    "PATH": "$PWD/bin:$PATH",
    "POSTGRESQL_HOME": "$PWD/devbox/postgresql",
    "POSTGRESQL_DATA_DIR": "$PWD/devbox/postgresql/data",
    "POSTGRESQL_UNIX_PORT": "/tmp/habit_postgresql.sock",
    "POSTGRESQL_PORT": "5432",
    "DEVBOX_COREPACK_ENABLED": "true",
    "SKIP_INIT": "false"
  }
}
